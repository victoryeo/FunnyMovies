<div class="container">
  <div class="row">
    <% @videos.each do |video| %>
      <div class="col-md-3 thumb video-thumb" data-video-id="<%= video.id %>">
        <div class="likes">
          <span class="likes-count">
            <%= video.likes %>
          </span>
          <span class="glyphicon glyphicon-heart"></span>
          <span class="likes-count">
            <img src="<%= video.thumbnail_url %>">"
          </span>
          <%= link_to 'Play', video %>
        </div>
      </div>
    <% end %>
  </div>
  <div class="row">
    <div class="col-md-12">
      <div class="pull-right">
        <%= link_to 'Upload new video', new_video_url, class: 'btn btn-primary' %>
      </div>
    </div>
  </div>
</div>
